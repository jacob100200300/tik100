      <div class="row">
        <div class="col ">
          <div class="services-filters component_filter_form_group component_filter_card mb-3">
            <div class="card">
              <div class="row">
                <div class="col-md-auto mb-3 mb-md-0">
                  <div class="component_filter_button">
                    <div class="dropdown">
                      <a class="btn btn-big-primary w-sm-auto w-100 dropdown-toggle" href="#" role="button" id="dropdown-category-filter-47" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <span class="fal fa-filter"></span>
                        <span class="services-filter__active-category" data-filter-active-category="true"></span>
                      </a>
                                            <div class="dropdown-menu"><a class="dropdown-item"  data-filter-category-id="All">All</a>{% for category in serviceCategory %}
